generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Url {
  id          Int      @id @default(autoincrement())
  originalUrl String
  shortUrl    String   @unique
  clicks      Int      @default(0)
  createdAt   DateTime @default(now())
}
